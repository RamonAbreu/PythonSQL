            #pos_hint:{'center_x':0.5, 'y':0.6}
            height:150
            canvas.before:
                Color:
                    rgba:self.background_color
                Rectangle:
                    size:self.size
                    pos:self.pos
#            Button:
#                id:btn_uctv2
