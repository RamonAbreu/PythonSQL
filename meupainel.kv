<PainelCentral>:
    name:"uct"
    id:central
    BoxLayout:
        padding:20
        spacing:20
        orientation:"vertical"
        size_hint_y:None
        pos_hint:{'center_x': 0.5,'y':0}
        size:root.width, root.height
        Label:
            background_color:0,0,1,1
            #size_hint:None,None
            size_hint_y:None
            height:'50dp'
            color: 1,0,0,1
            text: "Estação Central"
            font_size:30
            pos_hint:{'center_x': 0.5}
            canvas.before:
                Color:
                    rgba:self.background_color
                Rectangle:
                    size:self.size
                    pos:self.pos
        Label:
            id:id_uctv1
            color: 0,0,0.73,1
            background_color:1,1,1,1
            font_size:20
            size_hint_y:None
            #pos_hint:{'center_x':0.5, 'y':0.6}
            height:150
            canvas.before:
                Color:
                    rgba:self.background_color
                Rectangle:
                    size:self.size
                    pos:self.pos
