<PainelInicial>:
    id:id_inicial
    name:"inicial"
    Label:
        text:"Metrô de BH em tempo real"
        color:0,0,0,1
        background_color:(112/255,22/255,85/255,1)
        font_size:16
        size_hint_y:None
        pos_hint:{'center_x':0.5, 'top':1}
        height:'50dp'
        canvas.before:
            Color:
                rgba:self.background_color
            Rectangle:
                pos:self.pos
                size:self.size
    Label:
        text:"Clique na estação de embarque"
        font_size:20
        color:0,0,0,1
        background_color:0,0,1,1
        size_hint_y:None
        pos_hint:{'center_x':0.5, 'top':.90}
        height:'55dp'
        canvas.before:
            Color:
                rgba:self.background_color
            Rectangle:
                pos:self.pos
                size:self.size
    ScrollView
        name:'sv'
        id:id_sv
        size_hint_y:.83
        pos_hint:{'x':0, 'y':.11}
        do_scroll_x:False
        do_scroll_y:True
        scroll_type:['bars', 'content']
        bar_width:10
        GridLayout:
            cols:1
            background_color:0,0,0.4,1
            padding:30
            spacing:2
            size:root.width, root.height
            size_hint_y:None
            size_hint_x:None
            height:self.minimum_height
            row_default_height:180
            row_force_default:True
            Button:
                text: "Central"
                font_size:26
                size:100,100
                size_hint_y:None
                background_color:0,0.8,0.4,1
                on_release:
                    app.root.current = "uct"
                    root.manager.transition.direction = "left"
            Button:
                background_color:0,0.4,0.8,1
                text: "Eldorado"
                font_size:32
                size_hint_y:None
                size:100,100
                on_release:
                    app.root.current = "uel"
                    root.manager.transition.direction = "left"

