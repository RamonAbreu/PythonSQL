#                height:100
#                width:50
#                allow_stretch:True
        Button:
            size_hint:None, None
            size:[100,50]
            pos_hint:{'center_x':0.8}
            background_color:0,0.5,0.8,1
            text:""

            on_press:
                root.seta_press()

            on_release:
                app.root.current = "inicial"
                root.manager.transition.direction = "right"
                root.seta_release()
            Image:
                id:id_seta
                source:'seta.jfif'
                center_y:self.parent.center_y
                center_x:self.parent.center_x
                size_hint: None, None
                size:[100,50]


